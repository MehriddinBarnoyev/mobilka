{
  "name": "ProfitLab",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "android": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res && react-native run-android",
    "ios-bundle": "react-native bundle --entry-file index.js --bundle-output ios/main.jsbundle --dev false --platform ios --assets-dest ios",
    "ios-run": "npx react-native run-ios",
    "ios-sim": "npx react-native run-ios --simulator='iPhone 15 Pro'",
    "ios-archive": "cd ios && xcodebuild -workspace example.xcworkspace -scheme example -configuration Release -archivePath build/example.xcarchive clean archive -allowProvisioningUpdates",
    "ios-export": "cd ios && xcodebuild -exportArchive -archivePath build/example.xcarchive -exportOptionsPlist exportOptions.plist -exportPath build",
    "ios-zip": "cd ios/build && zip -r ProfitLab-iOS.zip ProfitLab.ipa",
    "ios-build": "npm run ios-bundle && npm run ios-archive && npm run ios-export && npm run ios-zip",
    "r": "adb reverse tcp:8081 tcp:8081",
    "dapk": "cd android && ./gradlew assembleDebug",
    "rapk": "cd android && ./gradlew assembleRelease",
    "id": "adb install -r android/app/build/outputs/apk/debug/app-debug.apk",
    "ir": "adb install -r android/app/build/outputs/apk/release/app-release.apk",
    "launch": "adb shell monkey -p com.assoodiq.devops 1",
    "alog": "react-native log-android",
    "postinstall": "patch-package",
    "raab": "cd android && ./gradlew bundleRelease"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.2",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-native-community/push-notification-ios": "^1.11.0",
    "@react-native-community/toolbar-android": "^0.2.1",
    "@react-native/virtualized-lists": "^0.82.0",
    "@react-navigation/bottom-tabs": "^6.6.1",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/native-stack": "^6.11.0",
    "axios": "^1.12.2",
    "deprecated-react-native-prop-types": "^2.3.0",
    "expo-asset": "^12.0.9",
    "expo-av": "~16.0.7",
    "expo-file-system": "^19.0.17",
    "intl": "^1.2.5",
    "lucide-react-native": "^0.525.0",
    "nativewind": "^4.2.1",
    "radio-buttons-react-native": "^1.0.4",
    "react": "19.0.0",
    "react-native": "0.79.0",
    "react-native-device-info": "^14.1.1",
    "react-native-dialogs": "^1.1.2",
    "react-native-gesture-handler": "^2.28.0",
    "react-native-in-app-updates": "^0.2.2",
    "react-native-localize": "^3.5.4",
    "react-native-orientation-locker": "^1.7.0",
    "react-native-privacy-snapshot": "^1.0.0",
    "react-native-reanimated": "^3.19.2",
    "react-native-safe-area-context": "^5.6.1",
    "react-native-screens": "^4.16.0",
    "react-native-svg": "^15.14.0",
    "react-native-vector-icons": "^10.3.0",
    "react-native-version-check": "^3.5.0",
    "react-native-video": "^6.17.0",
    "react-native-webview": "^13.16.0",
    "vdocipher-rn-bridge": "^1.21.4"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "18.0.0",
    "@react-native-community/cli-platform-android": "18.0.0",
    "@react-native-community/cli-platform-ios": "18.0.0",
    "@react-native/babel-preset": "0.79.0",
    "@react-native/eslint-config": "0.79.0",
    "@react-native/metro-config": "0.79.0",
    "@react-native/typescript-config": "0.79.0",
    "@types/jest": "^29.5.13",
    "@types/react": "^19.0.0",
    "@types/react-native-privacy-snapshot": "^1.0.2",
    "@types/react-native-vector-icons": "^6.4.18",
    "@types/react-native-version-check": "^3.4.8",
    "@types/react-test-renderer": "^19.0.0",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "patch-package": "^8.0.1",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "2.8.8",
    "react-test-renderer": "19.0.0",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
}
